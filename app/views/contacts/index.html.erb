<div class="page-header">
  <h1>Address Book <small>(codename Duchess)</small></h1>
</div>
<div class="container">

  <div class="contacts">
    <% @contacts.each do |contact| %>
      <h2><%= contact.first_name %> <%= contact.last_name %></h2>
      <p><%= contact.address %> <%= contact.city %>, <%= contact.state %> <%= contact.zip_code %></p>
    <% end %>

  </div>

</div>
