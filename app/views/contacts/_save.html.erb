$("ul.errors").html("")
<% if @contact.errors.any? %>
  <% @contact.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".contact-index").html("<%= escape_javascript(redirect_to artists_path) %>")
  $("#contact-modal").modal("hide")
<% end %>
